CREATE TABLE gyarto(adoszam INT PRIMARY KEY, nev CHAR(50), telephely VARCHAR(50));
CREATE TABLE termek(tkod INT PRIMARY KEY, nev CHAR(50), ear NUMERIC NOT NULL, gyarto INT FOREIGN KEY);
CREATE TABLE egysegek(aru NUMERIC FOREIGN KEY, db NUMERIC NOT NULL);
CREATE TABLE komponens(FOREIGN KEY Termek NUMERIC, FOREIGN KEY Alkatresz NUMERIC);
CREATE TABLE Alkatresz(akod NUMERIC, nev CHAR(50));
